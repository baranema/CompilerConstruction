extern int printf(char[] format, ...);

int main() {
    int i = 0;

    do {
        i++;
        for (int i = 0 to 3) {
            continue; // does nothing, just call continue inside the inner loop till it ends. should not affect outer loop!
            printf("bad bad print\n");
        }
        printf("%d\n", i);
    } while(i < 5);

    printf("done\n");
    return 0;
}
