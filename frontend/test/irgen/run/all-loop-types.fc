extern int printf(char[] format, ...);

int main() {
    int i = 3;

    printf("do while loop:\n");
    do {
        printf("%d\n", i);
        i--;
    } while(i > 0);

    printf("while loop:\n");
    while(i < 5){
        printf("%d\n", i);
        i++;
    }

    printf("for loop:\n");
    for(int j = 0 to 2){
        printf("%d\n", j);
    }

    printf("done\n");
    return 0;
}
