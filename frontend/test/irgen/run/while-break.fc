extern int printf(char[] format, ...);

int main() { //works
    int i = 0;
    while (i < 5) {
        if (i == 3)
            break;
        printf("%d\n", i);
        i += 1;
    }
    return 0;
}

int main() { //break breaks
    int i = 0;
    while (i < 5) {
        break;
        printf("%d\n", i);
        i += 1;
    }
    return 0;
}

